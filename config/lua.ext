[ExtensionFile]
EXTENSIONTYPE: lua
EXTENSIONS: *.lua
SEPS: !%&/()=?+-.,:;*^~><\\[]{}|\t\\\"'´`   
STRING: "
CHAR: '
LITERAL: \
LINECONTINUATION: \
